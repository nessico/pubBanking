{
  "runtime": "NetCore21",
  "swaggerGenerator": {
    "fromSwagger": {
      "url": "https://avocado-toast.wp6.pw/api.yaml"
    }
  },
  "codeGenerators": {
    "swaggerToTypeScriptClient": {
      "typeScriptVersion": 3.3,
      "operationGenerationMode": "MultipleClientsFromOperationId",
      "excludedTypeNames": ["PartialClient"],
      "output": "./src/models/api.ts"
    }
  }
}